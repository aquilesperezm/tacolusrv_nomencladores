<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <group name="data" numcolumns="12">
            <column name="id" display="" order="100">
                <widget type="text" fieldname="id" readonly="true"/>
            </column>
            <column name="comentarios" display="left" order="110">
                <widget type="textarea" fieldname="comentarios"/>
            </column>
            <column name="fecha_matriculacion" display="left" order="120">
                <widget type="date" fieldname="fecha_matriculacion" required="true"/>
            </column>
            <column name="matricula" display="left" order="130">
                <widget type="text" fieldname="matricula" maxlength="20" required="true"/>
            </column>
            <column name="num_chasis" display="left" order="140">
                <widget type="text" fieldname="num_chasis" maxlength="20" required="true"/>
            </column>
        </group>
    </columns>
</view>
